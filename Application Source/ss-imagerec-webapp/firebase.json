{
  "hosting": {
    "public": "dist",
    "rewrites": [{
      "source": "send",
      "function": "app"
      },
      {
        "source": "sendcached",
        "function": "app"
      },
      {
        "source": "sendmail",
        "function": "app"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"%RESOURCE_DIR%\" run lint"
    ]
  }
}
